<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.puthouse.WarehouseActivity">

    <com.rfid.pdaapp.views.TitleBar
        android:layout_width="match_parent"
        android:layout_height="45dp"
        app:title="收货入库" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_big_goods"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:background="@drawable/shape_theme_10"
            android:gravity="center"
            android:paddingTop="30dp"
            android:paddingBottom="30dp"
            android:text="大货退货入库"
            android:textColor="@color/white"
            android:textSize="@dimen/dp_big" />

        <TextView
            android:id="@+id/tv_stock_put"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:background="@drawable/shape_green_10"
            android:gravity="center"
            android:paddingTop="30dp"
            android:paddingBottom="30dp"
            android:text="库位上架"
            android:textColor="@color/white"
            android:textSize="@dimen/dp_big" />
    </LinearLayout>


</LinearLayout>
